{
  "bucket_interval": "20m",
  "default_datasource": "influx",
  "features": [
    {
      "field": "value",
      "measurement": "mydata",
      "metric": "avg",
      "name": "avg_value"
    },
    {
      "default": 0,
      "field": "value",
      "measurement": "mydata",
      "metric": "count",
      "name": "count_value"
    }
  ],
  "interval": 60,
  "max_evals": 10,
  "name": "timeseries-influx",
  "offset": 30,
  "span": 5,
  "max_threshold": 70,
  "min_threshold": 60,
  "type": "timeseries"
}
